{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/check-activation",
      "dest": "/api/check-activation.js"
    },
    {
      "src": "/api/check-status",
      "dest": "/api/check-status.js"
    },
    {
      "src": "/api/deactivate",
      "dest": "/api/deactivate.js"
    },
    {
      "src": "/api/update-expiry",
      "dest": "/api/update-expiry.js"
    }
  ]
}
